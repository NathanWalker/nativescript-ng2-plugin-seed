{
  "name": "nativescript-ng2-yourplugin",
  "version": "0.0.0",
  "description": "Your awesome NativeScript + Angular plugin.",
  "main": "yourplugin.js",
	"nativescript": {
		"platforms": {
			"android": "2.0.0",
      "ios": "2.0.0"
		}
	},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YourName/nativescript-ng2-yourplugin.git"
  },
  "keywords": [
    "angular2",
    "ng2",
    "nativescript"
  ],
  "author": "Your Name <yourname@yourdomain.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YourName/nativescript-ng2-yourplugin/issues"
  },
  "homepage": "https://github.com/YourName/nativescript-ng2-yourplugin#readme",
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo && cd src && tns emulate ios",
    "demo.android": "npm run preparedemo && cd src && tns emulate android",
    "test": "tsc && karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "npm run test",
    "docs": "typedoc --options typedoc.json src/app/app.ts",
	"repair": "cd src && tns plugin add .. && tns install",
    "postinstall": "typings install",
    "preparedemo": "npm run build && cd src && tns plugin remove nativescript-ng2-yourplugin && tns plugin add .. && tns install",
    "setup": "cd src && npm i"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
		"@angular/compiler": "2.0.0-rc.1",
		"@angular/core": "2.0.0-rc.1",
		"@angular/http": "2.0.0-rc.1",
		"@angular/platform-browser": "2.0.0-rc.1",
		"@angular/platform-browser-dynamic": "2.0.0-rc.1",
		"@angular/platform-server": "2.0.0-rc.1",
		"@angular/router": "2.0.0-rc.1",
		"@angular/router-deprecated": "2.0.0-rc.1",
		"es6-promise": "~3.1.2",
		"es6-shim": "^0.35.0",
		"nativescript-angular": "0.1.6",
		"parse5": "1.4.2",
		"punycode": "1.3.2",
		"querystring": "0.2.0",
		"reflect-metadata": "0.1.3",
		"rimraf": "^2.5.1",
		"rxjs": "5.0.0-beta.6",
		"tns-core-modules": "^2.0.1",
		"url": "0.10.3",
		"zone.js": "0.6.12"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "jasmine-core": "^2.3.4",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript-preprocessor": "^0.2.0",
    "node-sass": "^3.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.2",
    "systemjs-builder": "^0.15.17",
    "traceur": "^0.0.108",
    "tsconfig-lint": "^0.11.0",
    "tslint": "^3.10.2",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
